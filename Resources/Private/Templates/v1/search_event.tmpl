	<label for="event_end_day">###L_EVENT_EDIT_ENDDATE###:</label>
    <input type="text" value="###EVENT_END_DAY###" name="tx_cal_controller[end_day]" id="event_end_day" /><br />
	<label for="query">###L_SEARCH_STRING###:</label>
    <input type="text" value="###QUERY###" name="tx_cal_controller[query]" id="query" /><br />
    <input type="hidden" name="tx_cal_controller[offset]" value="0" />
	<input type="image" src="###IMG_PATH###/search.gif" name="tx_cal_controller[submit]" title="###DOSEARCH_LABEL###" value="Search" />
    </fieldset> 
</form>
