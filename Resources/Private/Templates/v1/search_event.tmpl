<h1>###HEADING###</h1>

<form action="###SEARCH_ACTION_URL###" method="post">
<fieldset>
<legend>###L_SEARCH###:</legend>   
	<input type="hidden" name="tx_cal_controller[getdate]" value="###GETDATE###" />
	<input type="hidden" size="15" name="tx_cal_controller[query]" value="###QUERY###" />
	<input type="hidden" size="15" name="tx_cal_controller[view]" value="###VIEW###" />
	<label for="tx_cal_controller_category_ids">###L_CATEGORY###:</label>
    <select name="tx_cal_controller[category][]" id="tx_cal_controller_category_ids" size="1">###CATEGORY_IDS###</select><br />
	<label for="tx_cal_controller_location_ids">###L_LOCATION###:</label>
    <select name="tx_cal_controller[location_ids][]" id="tx_cal_controller_location_ids" size="1">###LOCATION_IDS###</select><br />
